---
layout: raw
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <id>http://www.vortalcord.net/</id>
  <title>This Vortal Cord</title>
  <subtitle>Home base of Shizuka Kamishima</subtitle>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <link rel="self" href="http://www.vortalcord.net/atom.xml" />
  {% assign allposts = site.writing | sort: "date" | reverse %}
  {% for post in allposts limit:10 %}
  <entry>
    <id>{{ site.url }}{{ post.url }}</id>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author><name>Shizuka</name></author>
    <link href="{{ site.url }}{{ post.url }}" />
    <summary type="html">
      {% include meta/postexcerpt.html escape=true %}
    </summary>
  </entry>
  {% endfor %}
</feed>
